{
    "manifest_version": 2,
    "name": "TabList",
    "version": "0.5",
    "description": "カレントChromeウィンドウのタブ一覧を表示する",
    "permissions": [
        "tabs"
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action": {
        "default_title": "",
        "default_icon": "icon16.png",
        "default_popup": "popup.html"
    }
}